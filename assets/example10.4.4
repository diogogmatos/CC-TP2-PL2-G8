$ORIGIN 4.4.10.in-addr.arpa.
; Bind reverse
$TTL 1d
@       IN SOA Servidor1.local. admin.local. (
                2023012301 ; serial
                8h         ; refresh
                2h         ; retry
                4w         ; expire
                1h         ; minimum
              )
@       IN NS   Servidor1.local.
1       IN PTR  Servidor1
2       IN PTR  Servidor2
